>\n      <tr>\n        <td id=\"file-appdelegate-m-L29\" class=\"blob-num js-line-number\" data-line-number=\"29\"></td>\n        <td id=\"file-appdelegate-m-LC29\" class=\"blob-code blob-code-inner js-file-line\">\n</td>\n      </tr>\n      <tr>\n        <td id=\"file-appdelegate-m-L30\" class=\"blob-num js-line-number\" data-line-number=\"30\"></td>\n        <td id=\"file-appdelegate-m-LC30\" class=\"blob-code blob-code-inner js-file-line\">  self.<span class=\"pl-smi\">window</span> = [[UIWindow <span class=\"pl-c1\">alloc</span>] <span class=\"pl-c1\">initWithFrame:</span>[UIScreen <span class=\"pl-c1\">mainScreen</span>].bounds];</td>\n      </tr>\n      <tr>\n        <td id=\"file-appdelegate-m-L31\" class=\"blob-num js-line-number\" data-line-number=\"31\"></td>\n        <td id=\"file-appdelegate-m-LC31\" class=\"blob-code blob-code-inner js-file-line\">  UIViewController *rootViewController = [UIViewController <span class=\"pl-c1\">new</span>];</td>\n      </tr>\n      <tr>\n        <td id=\"file-appdelegate-m-L32\" class=\"blob-num js-line-number\" data-line-number=\"32\"></td>\n        <td id=\"file-appdelegate-m-LC32\" class=\"blob-code blob-code-inner js-file-line\">  rootViewController.<span class=\"pl-smi\">view</span> = rootView;</td>\n      </tr>\n      <tr>\n        <td id=\"file-appdelegate-m-L33\" class=\"blob-num js-line-number\" data-line-number=\"33\"></td>\n        <td id=\"file-appdelegate-m-LC33\" class=\"blob-code blob-code-inner js-file-line\">  self.<span class=\"pl-smi\">window</span>.<span class=\"pl-smi\">rootViewController</span> = rootViewController;</td>\n      </tr>\n      <tr>\n        <td id=\"file-appdelegate-m-L34\" class=\"blob-num js-line-number\" data-line-number=\"34\"></td>\n        <td id=\"file-appdelegate-m-LC34\" class=\"blob-code blob-code-inner js-file-line\">  [<span class=\"pl-c1\">self</span>.window <span class=\"pl-c1\">makeKeyAndVisible</span>];</td>\n      </tr>\n      <tr>\n        <td id=\"file-appdelegate-m-